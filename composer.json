{
    "name": "derasy/derasy-bundle",
    "description": "Derasy Rapid App Dev for React Angular and Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Donny",
            "email": "donny@nufaza.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "symfony/config": "^4.1",
        "symfony/dependency-injection": "^4.1",
        "symfony/http-kernel": "^4.1"
    },
    "autoload": {
        "psr-4": {
