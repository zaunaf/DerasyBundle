    "autoload-dev": {
        "psr-4": {
            "Derasy\\DerasyBundle\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.2@dev",
        "symfony/framework-bundle": "^4.2@dev",
        "symfony/browser-kit": "^4.2@dev",
        "propel/propel": "dev-master",
        "propel/propel-bundle": "4.0.x-dev"
    }
}